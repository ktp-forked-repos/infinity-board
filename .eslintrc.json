{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "class-methods-use-this": "warn",
    "import/no-extraneous-dependencies": "off",
    "jsx-a11y/no-static-element-interactions": "warn",
    "no-param-reassign": "warn",
    "no-unused-vars": "warn",
    "no-plusplus": "off",
    "react/no-unused-state": "warn"
  }
}